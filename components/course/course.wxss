/* components/course/course.wxss */

.course-item-container-history {
  overflow: hidden;
  position: relative;
  border-radius: 8rpx;
  margin: 20rpx;
}

.course-item-container-history .cover {
  width: 710rpx;
  height: 246rpx;
  vertical-align: bottom;
  background-position: center center;
}

.course-item-container-history .info {
  position: absolute;
  left: 32rpx;
  top: 40rpx;
  height: 166rpx;
  text-shadow: 2rpx 3rpx rgba(0, 0, 0, 0.2);
}

.course-item-container-history .time {
  font-size: 36rpx;
  font-weight: bold;
  margin-left: 0rpx;
  margin-top: 0rpx;
  line-height: 42rpx;
}

.course-item-container-history .avatar {
  position: absolute;
  left: 0rpx;
  bottom: 0rpx;
  width: 100rpx;
  height: 100rpx;
  border-width: 2rpx;
  border-color: white;
  border-radius: 10rpx;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  background-image: url("data:image/png;base64,")
}

.course-item-container-history .avatar image {
  width: 100%;
  height: 100%;
  overflow: hidden;
  border-width: 2rpx;
  border-color: white;
  border-radius: 10rpx;
}

.course-item-container-history .name {
  font-size: 30rpx;
  position: relative;
  line-height: 42rpx;
  padding-left: 116rpx;
  margin-top: 24rpx;
}

.course-item-container-history .name::before {
  content: '';
  position: absolute;
  left: 110rpx;
  top: 0;
  width: 6rpx;
  height: 42rpx;
  background-color: #4abdcc;
}

.course-item-container-history .chapter {
  position: absolute;
  font-size: 30rpx;
  line-height: 42rpx;
  left: 116rpx;
  bottom: 0rpx;
  z-index: 2;
}

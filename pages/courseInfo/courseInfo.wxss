/* pages/courseInfo/courseInfo.wxss */

.chapter-medias {
  width: 750rpx;
  height: 500rpx;
}

.chapter-medias .media-container {
  width: 500rpx;
  height: 416rpx;
  background-repeat: no-repeat;
  background-size: 488rpx 416rpx;
  background-position: center;
  background-image: url('data:image/png;base64,');
}

.chapter-medias .video {
  position: relative;
  top: 122rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.chapter-medias .audio {
  position: relative;
  top: 122rpx;
  display: flex;
  justify-content: center;
  align-items: flex-end;
}

.chapter-medias .audio .trumpet {
  position: absolute;
  left: 44rpx;
  top: 52rpx;
}
.chapter-medias .audio .control {
  position: absolute;
  right: 88rpx;
  top: 88rpx;
}
.chapter-medias .audio .progress {
  color: white;
  font-size: 36rpx;
  position: relative;
  top: 200rpx;
}

.chapter-medias .video .video-image {
  width: 456rpx;
  height: 264rpx;
  border-radius: 60rpx;
}

.chapter-info {
  width: 710rpx;
  height: 246rpx;
  margin: 20rpx;
}

.chapter-info .cover {
  width: 710rpx;
  height: 246rpx;
  vertical-align: bottom;
  background-position: center center;
}

.chapter-info view {
  font-size: 30rpx;
  color: white;
  position: relative;
  left: 40rpx;
}

.chapter-info .chapter {
  top: -200rpx;
}

.chapter-info .time {
  top: -180rpx;
}

.chapter-info .target {
  top: -160rpx;
}

.words {
  margin: 20rpx;
  font-size: 30rpx;
  color: black;
}

.words .content {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}

.words .content .word {
  margin-right: 20rpx;
  margin-top: 20rpx;
  padding: 20rpx;
  background-color: red;
  border-radius: 20rpx;
}
